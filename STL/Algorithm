#include <iostream>
#include<algorithm>
#include<vector>

using namespace std;

int main(){

vector<int> v;
v.push_back(5);
v.push_back(17);
v.push_back(10);
v.push_back(15);
sort(v.begin(), v.end());
cout<<binary_search(v.begin(), v.end(), 10)<<endl;

int a = 5;
int b = 7;
cout<<max(a,b)<<endl;
cout<<min(a,b)<<endl;
swap(a,b);
cout<<a<<endl;

string s = "dhairya";
reverse(s.begin(), s.end());
cout<<s;

return 0;

}
