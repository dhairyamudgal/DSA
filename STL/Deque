#include <iostream>
#include<deque>
using namespace std;

int main(){


deque<int> d;
d.push_front(1);
d.push_front(2);
for(int i=0;i<d.size();i++){
    cout<<d[i]<<" ";
}
cout<<"\n";
d.pop_front();
for(int i=0;i<d.size();i++){
    cout<<d[i]<<" ";
}

d.push_front(2);
d.push_front(7);
cout<<endl<<"Front"<<d.front()<<endl;
cout<<"BAck"<<d.back()<<endl;
cout<<"Is empty?"<<d.empty()<<endl;
}